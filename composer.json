{
    "name": "superdweebie/doctrine-extensions-module",
    "description": "Zend Framework 2 Module that provides Sds DoctrineExtensions functionality",
    "type": "library",
    "keywords": [
        "doctrine",
        "zf2"
    ],
    "homepage": "http://github.com/superdweebie/doctrineExtensionsModule",
    "authors": [
        {
            "name": "Tim Roediger",
            "email": "superdweebie@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "superdweebie/doctrine-extensions":   "~0.3",
        "superdweebie/exception-module":      "~0.3",
        "doctrine/doctrine-mongo-odm-module": "dev-master",
        "zendframework/zend-servicemanager":  "~2.1",
        "zendframework/zend-modulemanager":   "~2.1",
        "zendframework/zend-mvc":             "~2.1",
        "zendframework/zend-http":            "~2.1",
        "zendframework/zend-view":            "~2.1",
        "zendframework/zend-stdlib":          "~2.1"
    },
    "require-dev": {
        "zendframework/zend-test":            "~2.1"
    },
    "autoload": {
        "psr-0": {
            "Sds\\DoctrineExtensionsModule": "src/"
        }
    },
    "config": {
        "github-protocols": ["http", "https", "git"]
    }
}